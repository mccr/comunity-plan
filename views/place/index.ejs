<h1><%= title %></h1>
<p>Welcome to <%= title %></p>

<div id="map"></div>
<div class="row">
  <div class="col-sm-6 col-md-4 trip-panel" >
    <% for (i=0; i < places.length; i++) { %>
      <div class="thumbnail">
        <img src="<%= places[i].picPath %>.png" alt="">
        <div class="caption">
          <h3><%= places[i].place_id %></h3>
          <p><%= places[i].description %></p>
        </div>
      </div>
    <% } %>
  </div>
</div>

<script type="text/javascript">
 var myPlaces = <%- JSON.stringify(places) %>;
 </script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDiaRwHqB7k_4QpmPWiPdXWAAutNW_wiC4"></script>
<script type="text/javascript" src="/javascripts/main.js"></script>
